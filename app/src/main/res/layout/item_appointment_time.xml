                    android:background="@drawable/bg_gray_circle"
                    android:ellipsize="end"
                    android:fontFamily="@font/sharp_sans_medium"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:maxLines="1"
                    android:padding="@dimen/_2sdp"
                    android:text="AL"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_7sdp" />

                <TextView
                    android:id="@+id/tvTimeFour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/sharp_sans_medium"
                    android:gravity="center"
                    android:text="09:00 AM"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_11sdp" />

            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlParentTimeFive"
            android:layout_width="0dp"
            android:layout_height="@dimen/_40sdp"
            android:layout_weight="1"
            android:background="@drawable/bg_white_with_green_border"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/llTimeAndNameFive"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvNameFive"
                    android:layout_width="@dimen/_15sdp"
