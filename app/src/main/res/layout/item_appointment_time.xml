<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <LinearLayout
        android:id="@+id/linearAppointment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10sdp"
        android:orientation="horizontal">

        <RelativeLayout
            android:id="@+id/rlParentTimeOne"
            android:layout_width="0dp"
            android:layout_height="@dimen/_40sdp"
            android:layout_marginRight="@dimen/_4sdp"
            android:layout_weight="1"
            android:background="@drawable/bg_white_with_green_border">

            <LinearLayout
                android:id="@+id/llTimeAndNameOne"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvNameOne"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginRight="@dimen/_3sdp"
                    android:background="@drawable/bg_gray_circle"
                    android:ellipsize="end"
                    android:fontFamily="@font/sharp_sans_medium"
                    android:gravity="center"
                    android:layout_gravity="center"
                    android:maxLines="1"
                    android:padding="@dimen/_2sdp"
                    android:text="AL"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_7sdp" />

                <TextView
                    android:id="@+id/tvTimeOne"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/sharp_sans_medium"
                    android:gravity="center"
                    android:text="09:00 AM"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_11sdp" />

            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlParentTimeTwo"
            android:layout_width="0dp"
            android:layout_height="@dimen/_40sdp"
            android:layout_marginRight="@dimen/_4sdp"
            android:layout_weight="1"
            android:background="@drawable/bg_white_with_green_border">

            <LinearLayout
                android:id="@+id/llTimeAndNameTwo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvNameTwo"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginRight="@dimen/_3sdp"
                    android:background="@drawable/bg_gray_circle"
                    android:ellipsize="end"
                    android:layout_gravity="center"
                    android:fontFamily="@font/sharp_sans_medium"
                    android:gravity="center"
                    android:maxLines="1"
                    android:padding="@dimen/_2sdp"
                    android:text="AL"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_7sdp" />

                <TextView
                    android:id="@+id/tvTimeTwo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/sharp_sans_medium"
                    android:gravity="center"
                    android:text="09:00 AM"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_11sdp" />

            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rlParentTimeThree"
            android:layout_width="0dp"
            android:layout_height="@dimen/_40sdp"
            android:layout_marginRight="@dimen/_4sdp"
